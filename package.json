{
  "name": "valence-audio-site",
  "version": "0.2.0",
  "private": false,
  "license": "MIT",
  "homepage": "http://valence.audio/",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-jest": "19.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "codecov": "2.1.0",
    "css-loader": "0.28.0",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.11.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "2.28.0",
    "istanbul": "0.4.5",
    "jest": "19.0.2",
    "jest-cli": "22.2.1",
    "jest-css-modules": "1.1.0",
    "json-loader": "0.5.4",
    "postcss-loader": "1.3.3",
    "postcss-svgo": "2.1.6",
    "node-sass": "^4.4.0",
    "object-assign": "4.1.1",
    "path-exists": "3.0.0",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-svg-loader": "1.1.1",
    "react-test-renderer": "15.5.4",
    "sinon": "2.1.0",
    "style-loader": "0.16.1",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "svgo": "0.7.2",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2",
    "yarn": "0.22.0"
  },
  "dependencies": {
    "react": "15.5.4",
    "react-dom": "15.5.4"
  },
  "scripts": {
    "deploy": "yarn run clean:full && yarn run build && gh-pages -d build",
    "clean": "rm -rf build/",
    "clean:full": "rm -rf build/ coverage/ node_modules/",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "build:development": "NODE_ENV=development ./node_modules/.bin/webpack",
    "rebuild": "npm run clean && npm run build",
    "lint:javascript": "./node_modules/.bin/eslint ./",
    "lint:stylus": "./node_modules/.bin/stylint ./source/",
    "lint:fix": "yarn run lint --fix",
    "lint": "yarn run lint:javascript && yarn run lint:stylus",
    "test": "./node_modules/.bin/jest --config .jestrc.json",
    "coverage": "./node_modules/.bin/codecov",
    "watch": "webpack-dev-server",
    "start": "yarn run clean && yarn run build:development && yarn watch"
  }
}
